{
  "name": "paksuco/currency",
  "description": "WIP - A currency management solution",
  "keywords": [
    "laravel",
    "currency",
    "management"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Taha PAKSU",
      "email": "tpaksu@gmail.com",
      "homepage": "https://rettica.com"
    }
  ],
  "require": {
    "php": ">=7.0.0",
    "illuminate/support": "^7.0|^8.0",
    "illuminate/config": "^7.0|^8.0",
    "illuminate/routing": "^7.0|^8.0",
    "livewire/livewire": "^2.0",
    "jenssegers/agent": "^2.6.4",
    "paksuco/settings": "@dev"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.3"
  },
  "autoload": {
    "psr-4": {
      "Paksuco\\Currency\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Paksuco\\Currency\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Paksuco\\Currency\\CurrencyServiceProvider"
      ],
      "aliases": {
        "Package": "Paksuco\\Currency\\Facade"
      }
    }
  },
  "repositories": [
    {
      "type": "path",
      "url": "../settings",
      "options": {
        "symlink": true
      }
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true
}
